{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prestart": "npm run fix-lucide-icons",
    "preandroid": "npm run fix-lucide-icons",
    "preios": "npm run fix-lucide-icons",
    "preweb": "npm run fix-lucide-icons",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --config config/eslint.config.mjs",
    "type-check": "tsc --noEmit",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --runInBand --coverage",
    "test:integration": "npm test -- --runInBand tests/integration",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "fix-lucide-icons": "node scripts/fix-lucide-esm-icons.js",
    "postinstall": "npm run fix-lucide-icons"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "expo": "^54.0.21",
    "expo-application": "^7.0.7",
    "expo-auth-session": "^7.0.9",
    "expo-dev-client": "~6.0.19",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.9",
    "firebase": "^11.10.0",
    "lucide-react-native": "^0.554.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-svg": "^15.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/detox": "^18.1.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~54.0.6",
    "detox": "^20.12.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.46.2"
  },
  "private": true
}
